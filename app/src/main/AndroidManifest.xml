<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.topicosavanzados.topicos">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PersonasActivity"
            android:label="Personas"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".PlacasActivity" />
        <activity android:name=".AutosActivity"></activity>
        <activity
            android:name=".MostrarActivity"
            android:label="@string/title_activity_mostrar"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>